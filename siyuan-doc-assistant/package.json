{
  "name": "doc-assistant",
  "version": "0.0.1",
  "type": "module",
  "description": "Doc Assistant plugin for Siyuan (https://b3log.org/siyuan)",
  "author": "",
  "license": "MIT",
  "homepage": "",
  "repository": "",
  "scripts": {
    "dev": "vite build --watch",
    "release": "node ./release.js",
    "release:manual": "node ./release.js --mode=manual",
    "release:patch": "node ./release.js --mode=patch",
    "release:minor": "node ./release.js --mode=minor",
    "release:major": "node ./release.js --mode=major",
    "build": "vite build",
    "typecheck:strict": "tsc -p tsconfig.strict.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "@antfu/eslint-config": "^4.8.1",
    "@types/node": "^20.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-perfectionist": "^4.2.0",
    "fast-glob": "^3.2.12",
    "glob": "^11.0.1",
    "jsdom": "^28.1.0",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.62.1",
    "siyuan": "1.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^6.2.1",
    "vitest": "^3.2.4",
    "vite-plugin-static-copy": "^0.15.0",
    "vite-plugin-zip-pack": "^1.0.5"
  }
}
